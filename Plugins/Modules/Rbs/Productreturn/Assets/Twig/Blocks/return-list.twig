{% block rbsProductreturnReturnListUse %}
	{% use 'Rbs_Order/order-templates.twig' %}
	{% use 'Rbs_Productreturn/return-templates.twig' %}
{% endblock rbsProductreturnReturnListUse %}

{% block rbsProductreturnReturnListMain %}
	{% set returns = productReturnsData %}
	{% set showOrderLink = true %}
	{% if returns | length %}
		{{ block('rbsProductreturnList') }}
	{% else %}
		{{ i18n('m.rbs.productreturn.front.no_return_yet', ['ucf']) }}
	{% endif %}
{% endblock rbsProductreturnReturnListMain %}

{% block rbsProductreturnReturnListFooter %}
	{% include 'Rbs_Generic/pagination.twig' with {'pageCount': pagination.pageCount, 'pageNumber': pagination.pageNumber, 'blockId': blockId} %}
{% endblock rbsProductreturnReturnListFooter %}